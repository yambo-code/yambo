ac_compiler_gnu=$ac_cv_f77_compiler_gnu

     if test "$ac_success_extra" = "yes"; then
	ac_cv_f77_mangling="$ac_case case"
	if test -z "$ac_underscore"; then
	   ac_cv_f77_mangling="$ac_cv_f77_mangling, no underscore"
