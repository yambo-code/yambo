$as_echo "to be compiled" >&6; }
      ac_config_files="$ac_config_files lib/iotk/make_iotk.inc:lib/iotk/make_iotk.inc.in"

    else
      compile_iotk="no"
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: already compiled" >&5
$as_echo "already compiled" >&6; }
    fi
  fi
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for IOTK library" >&5
$as_echo_n "checking for IOTK library... " >&6; }
