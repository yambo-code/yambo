    #
    if test -d "$with_iotk_path" ;   then { $as_echo "$as_me:${as_lineno-$LINENO}: checking for IOTK in $with_iotk_path" >&5
$as_echo_n "checking for IOTK in $with_iotk_path... " >&6; } ; fi
    if test -d "$with_iotk_libdir" ; then { $as_echo "$as_me:${as_lineno-$LINENO}: checking for IOTK in $with_iotk_libdir" >&5
$as_echo_n "checking for IOTK in $with_iotk_libdir... " >&6; } ; fi
